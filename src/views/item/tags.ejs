<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><%= name %> tags</title>
</head>
<body>
	<h1><%= name %></h1>
	<p>tags:</p>
	  <ul>
	  <% tags.forEach(tag => { %>
	    <li id='<%= tag.id %>'><a href="/list/<%= tag.name %>/top"><%= tag.name %></a>
		<button onclick="vote(['<%= id %>', '<%= tag.id %>'], 1, 'tag', 'vote-<%= tag.id %>' )">↑</button>
		<button onclick="vote(['<%= id %>', '<%= tag.id %>'], 0, 'tag', 'vote-<%= tag.id %>' )">↓</button>
		<span id="vote-<%= tag.id %>"></span></li>
	  <% }) %>
	</ul>
	<a href=".">back</a><br>
	<a href="/">search</a>
<script src="/js/vote.js"></script>
</body>
</html>
